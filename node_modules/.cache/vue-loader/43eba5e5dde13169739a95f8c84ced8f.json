\n        <button @click=\"clickButton\">генерировать пароль</button>\n        <h3>{{password}}</h3>\n    </div>\n</template>\n\n<script>\nexport default {\n    name: 'User',\n    data : function() {\n        return {\n            ws : null,\n            password : null,\n            id : null,\n            virtualMouse : false\n        }\n    },\n    methods : {\n        clickButton : function() {\n            this.password = this.generateID();\n            this.ws.send(JSON.stringify({type : 'newpass', pass : this.password}))\n        },\n        generateID : function () {\n            return Math.floor((1 + Math.random()) * 0x10000)\n            .toString(16)\n            .substring(1);\n        },\n        SetCoords : function(arr) {\n            !this.virtualMouse ? this.virtualMouse = true : false;\n            self.$refs.cursor.style.top = arr[1] + 'px';\n            self.$refs.cursor.style.left = arr[0] + 'px';\n            arr[2] ? self.$refs.user.style.width = arr[2] + 'px' : false\n        }\n    },\n    mounted : function() {\n        const self = this;\n        this.ws = new WebSocket('ws://localhost:8081');\n        this.ws.onmessage = function( message ) {\n            console.dir(message);\n            var res = JSON.parse(message.data);\n            let type = res[0];\n            switch (type) {\n                case 'confirm' : self.id = res[1];break;\n                case 'info' : console.dir(res[1]);break;\n                case 'cords' : SetCoords(res[1]); break;\n            }\n        }\n  }\n}\n</script>\n\n<style scoped>\n    h3 {\n        font-size: 15px;\n        padding-left: 15px;\n    }\n\n    .cursor {\n        width: 15px;\n        height: 15px;\n        position: fixed;\n        border-radius: 50%;\n        background-color: green;\n    }\n\n    .user {\n        position: absolute;\n        width: 100%;\n        height: 300%;\n        top: 0;\n        left: 0;\n    }\n</style>\n\n\n"]}]}